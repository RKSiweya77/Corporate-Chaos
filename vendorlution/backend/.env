# --- Ozow (staging) ---
OZOW_API_BASE=https://stagingapi.ozow.com
OZOW_API_KEY=YOUR_STAGING_API_KEY
OZOW_SITE_CODE=YOUR_STAGING_SITE_CODE
OZOW_PRIVATE_KEY=YOUR_STAGING_PRIVATE_KEY
OZOW_IS_TEST=true

# Public callback URLs (must be internet-reachable)
# Replace <your-host> with your tunnel or staging host
OZOW_SUCCESS_URL=https://<your-host>/payments/ozow/success/
OZOW_CANCEL_URL=https://<your-host>/payments/ozow/cancel/
OZOW_ERROR_URL=https://<your-host>/payments/ozow/error/
OZOW_NOTIFY_URL=https://<your-host>/payments/ozow/notify/